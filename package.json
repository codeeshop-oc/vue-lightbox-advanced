{
  "name": "vue-lightbox-advanced",
  "version": "2.0.0",
  "description": "Vue Lightbox Advanced Photo Grid component for Vue.js",
  "main": "dist/vue-lightbox-advanced.ssr.js",
  "browser": "dist/vue-lightbox-advanced.esm.js",
  "module": "dist/vue-lightbox-advanced.esm.js",
  "unpkg": "dist/vue-lightbox-advanced.min.js",
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "grid view",
    "collage",
    "lightbox",
    "vue-lightbox",
    "vue-lightbox-advanced",
    "lightbox advanced",
    "lightbox events",
    "shuffle images",
    "shuffle"
  ],
  "author": "Code-E-Shop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeeshop-oc/vue-lightbox-advanced/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeeshop-oc/vue-lightbox-advanced.git"
  },
  "homepage": "https://codeeshop-oc.github.io/vue-lightbox-advanced/",    
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
